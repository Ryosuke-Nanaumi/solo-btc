{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "db:migrate": "knex --knexfile ./db/knexfile.js migrate:latest",
    "db:migrate-make": "knex --knexfile ./db/knexfile.js migrate:make",
    "db:seed": "knex --knexfile ./db/knexfile.js seed:run",
    "db:seed-make": "knex --knexfile ./db/knexfile.js seed:make",
    "db:reset": "knex --knexfile ./db/knexfile.js migrate:rollback --all && knex --knexfile ./db/knexfile.js migrate:latest && knex --knexfile ./db/knexfile.js seed:run"
  },
  "keywords": [],
